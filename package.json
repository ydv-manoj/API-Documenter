{
  "name": "api-documenter",
  "version": "1.0.0",
  "description": "Automatically scan your codebase and generate interactive Swagger UI documentation for your APIs",
  "main": "src/cli/index.js",
  "bin": {
    "api-documenter": "./bin/api-documenter"
  },
  "scripts": {
    "start": "node src/cli/index.js",
