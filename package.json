  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.27.5",
    "@babel/traverse": "^7.27.4",
    "@babel/types": "^7.27.6",
    "acorn": "^8.10.0",
    "acorn-walk": "^8.2.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.6.0",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "esprima": "^4.0.1",
    "esprima-walk": "^0.1.0",
    "express": "^4.21.2",
    "fs-extra": "^11.3.0",
    "glob": "^10.4.5",
